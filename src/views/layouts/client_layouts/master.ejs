	<body>
		<div id="page">
			<!-- #site-header -->
			<%- include('./header') %>
			<!-- #end-site-header -->

			<!-- #main -->
			<%- include('./content') %>

			<!-- #footer -->
			<%- include('footer') %>
			<!-- #end-footer -->
		</div>

		<!-- javascript -->
		<%- include('./script') %> <%- defineContent('js') %>
